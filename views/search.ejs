<!DOCTYPE html>
<html>
<head>
	<title>Bowling Ball Search</title>
	<link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
	<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.10/angular.min.js"></script>
	<script src="http://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0rc1/angular-route.min.js"></script>
	<script src="/javascripts/angularApp.js"></script>
</head>
<body ng-app="seafordBowlingApp" ng-controller="SearchCtrl">
	<div id="header">
		<div id="box">
		</div>
		<img id="logo" src="/images/logo.png" alt="">
		<div id="box-border">
		</div>
	</div>
	<a href="home"><button id="back-top" type="button">Back</button></a>
	<input ng-model="boxer" id="search-box" type="text" name="search" value="" placeholder="Search Clients">
	<button ng-click="searchFor()" id="search-button" type="button" name="search">Search</button>
	
	<h1 id="name-header">Name</h1>
	<h1 id="ball-header">Ball Type</h1>
	<h1 id="date-header">Date</h1>
	<ul id="list">
		<div ng-repeat="customer in customers | orderBy:'name'">
			<li>
				<button ng-click="editCustomer()" class="searched-field" type="button"><span class="searched-name">{{customer.name}}</span><span class="searched-ball-type">{{customer.ball}}</span><span class="searched-date">{{customer.date}}</span></button>
			</li>
		</div>
	</ul>
	
</body>
</html>