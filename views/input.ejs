						<td class="cell"><input ng-model="bhsize4" class="table-data" id="bhsize4" type="text" name="bhsize4"></td>
						<td class="cell"><input ng-model="bhdepth4" class="table-data" id="bhdepth4" type="text" name="bhdepth4"></td>
						<td class="cell"><input ng-model="surface4" class="table-data" id="surface4" type="text" name="surface4"><br></td>
				</tr>
				<tr class="cell">
						<td class="cell"><input ng-model="ball5" class="table-data" id="ball5" type="text" name="ball5"></td>
						<td class="cell"><input ng-model="weight5" class="table-data" id="weight5" type="text" name="weight5"></td>
						<td class="cell"><input ng-model="pin5" class="table-data" id="pin5" type="text" name="pin5"></td>
						<td class="cell"><input ng-model="layout5" class="table-data" id="layout5" type="text" name="layout5"></td>
						<td class="cell"><input ng-model="bhpos5" class="table-data" id="bhpos5" type="text" name="bhpos5"></td>
						<td class="cell"><input ng-model="bhsize5" class="table-data" id="bhsize5" type="text" name="bhsize5"></td>
						<td class="cell"><input ng-model="bhdepth5" class="table-data" id="bhdepth5" type="text" name="bhdepth5"></td>
						<td class="cell"><input ng-model="surface5" class="table-data" id="surface5" type="text" name="surface5"><br></td>
				</tr>
				<tr class="cell">
						<td class="cell"><input ng-model="ball6" class="table-data" id="ball6" type="text" name="ball6"></td>
						<td class="cell"><input ng-model="weight6" class="table-data" id="weight6" type="text" name="weight6"></td>
