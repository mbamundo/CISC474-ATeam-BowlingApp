<body ng-app="seafordBowlingApp" ng-controller="MainCtrl">
	<div id="container">
		<div id="left-side">
			<img id="diagram" src="/images/bowlingInput.png" alt="">
			<form>
				<input ng-model="frac1" class="input" id="frac1" type="text" name="frac1" placeholder="Fraction"><br> <!--<span id="frac1">{{customer.frac1}}</span>-->
				<input ng-model="frac2" class="input" id="frac2" type="text" name="frac2" placeholder="Fraction"><br>
				<input ng-model="frac3" class="input" id="frac3" type="text" name="frac3" placeholder="Fraction"><br>
				<input ng-model="frac4" class="input" id="frac4" type="text" name="frac4" placeholder="Fraction"><br>
				<input ng-model="frac5" class="input" id="frac5" type="text" name="frac5" placeholder="Fraction"><br>
				<input ng-model="frac6" class="input" id="frac6" type="text" name="frac6" placeholder="Fraction"><br>
				<input ng-model="frac7" class="input" id="frac7" type="text" name="frac7" placeholder="Fraction"><br>
				<input ng-model="frac8" class="input" id="frac8" type="text" name="frac8" placeholder="Fraction"><br>
				<input ng-model="frac9" class="input" id="frac9" type="text" name="frac9" placeholder="Fraction"><br>
				<input ng-model="deci1" class="input" id="deci1" type="text" name="deci1" placeholder="Decimal"><br>
				<input ng-model="deci2" class="input" id="deci2" type="text" name="deci2" placeholder="Decimal"><br>
				<input ng-model="text1" class="free-text" id="text1" type="text" name="text1" placeholder="Free Text"><br>
				<input ng-model="text2" class="free-text" id="text2" type="text" name="text2" placeholder="Free Text"><br>
				<input ng-model="text3" class="free-text" id="text3" type="text" name="text3" placeholder="Free Text"><br>
			</form> 
			<p class="label" id="oval1">Oval Cut</P>
			<p class="label" id="oval2">Oval Cut</p>
			<form class="checkboxes" id="check1">
				<input ng-model="cut1" class="check-margin" type="checkbox" name="cut1" value="Cut to Cut"><br>
				<input ng-model="fspan1" type="checkbox" name="fspan1" value="Full Span">
			</form> 
			<form class="checkboxes" id="check2">
				<input ng-model="cut2" class="check-margin" type="checkbox" name="cut2" value="Cut to Cut"><br>
				<input ng-model="fspan2" type="checkbox" name="fspan2" value="Full Span">
			</form>
			<p class="label" id="cut1">Cut to Cut</p>
			<p class="label" id="full1">Full Span</p>
			<p class="label" id="cut2">Cut to Cut</p>
			<p class="label" id="full2">Full Span</p>
			<form class="radio-buttons" id="radio1">
				<input ng-model="radio1" class="button-margin-vert" type="radio" name="arrow1" value="up"><br>
				<input ng-model="radio1" type="radio" name="arrow1" value="down">
			</form>
			<form class="radio-buttons" id="radio2">
				<input ng-model="radio2" class="button-margin-horiz" type="radio" name="arrow2" value="left">
				<input ng-model="radio2" type="radio" name="arrow2" value="right">
			</form>
			<form class="radio-buttons" id="radio3">
				<input ng-model="radio3" class="button-margin-horiz" type="radio" name="arrow3" value="left">
				<input ng-model="radio3" type="radio" name="arrow3" value="right">
			</form>
			<form class="radio-buttons" id="radio4">
				<input ng-model="radio4" class="button-margin-vert" type="radio" name="arrow4" value="up"><br>
				<input ng-model="radio4" type="radio" name="arrow4" value="down">
			</form>
			<form class="radio-buttons" id="radio5">
				<input ng-model="radio5" class="button-margin-vert" type="radio" name="arrow5" value="up"><br>
				<input ng-model="radio5" type="radio" name="arrow5" value="down">
			</form>
			<form class="radio-buttons" id="radio6">
				<input ng-model="radio6" class="button-margin-horiz" type="radio" name="arrow6" value="left">
				<input ng-model="radio6" type="radio" name="arrow6" value="right">
			</form>
		</div>
		<div id="right-side">
			<form>
				Name: <input  ng-model="name" class="info" id="name" type="text" name="name" placeholder="John Doe">
				Date: <input ng-model="date" class="info" id="date" type="text" name="date" placeholder="mm/dd/yyyy"><br>
				Address: <input ng-model="address" class="info" id="address" type="text" name="address" placeholder="123 Main St, Apt. 456"><br>
				City: <input ng-model="city" class="info" id="city" type="text" name="city" placeholder="City">
				State: <input ng-model="state" class="info" id="state" type="text" name="state" placeholder="ST">
				Zip: <input ng-model="zip" class="info" id="zip" type="text" name="zip" placeholder="12345"><br>
				Phone: <input ng-model="phone" class="info" id="phone" type="text" name="phone" placeholder="(xxx) xxx-xxxx">
				Cell: <input ng-model="cell" class="info" id="cell" type="text" name="cell" placeholder="(xxx) xxx-xxxx"><br>
				Email: <input ng-model="email" class="info" id="email" type="text" name="email" placeholder="johndoe@example.com"><br><br>
				RH: <input ng-model="rh" class="info" id="rh" type="text" name="rh" placeholder="RH">
				LH: <input ng-model="lh" class="info" id="lh" type="text" name="lh" placeholder="LH">
				Conv: <input ng-model="conv" class="info" id="conv" type="text" name="conv" placeholder="Conv">
				FT: <input ng-model="ft" class="info" id="ft" type="text" name="ft" placeholder="FT"><br>
				Ball: <input ng-model="ball" class="info" id="ball" type="text" name="ball" placeholder="Ball">
				Wgt: <input ng-model="wgt" class="info" id="wgt" type="text" name="wgt" placeholder="Wgt">
				Pin: <input ng-model="pin" class="info" id="pin" type="text" name="pin" placeholder="Pin"><br>
				Layout: <input ng-model="layout" class="info" id="layout" type="text" name="layout" placeholder="Layout">
				Surface: <input ng-model="surface" class="info" id="surface" type="text" name="surface" placeholder="Surface"><br>
				BH Position: <input ng-model="bhPosition" class="info" id="bhpos" type="text" name="bhpos" placeholder="BH Position">
				Size: <input ng-model="size" class="info" id="size" type="text" name="size" placeholder="Size">
				Depth: <input ng-model="depth" class="info" id="depth" type="text" name="depth" placeholder="Depth"><br><br>
				PAP: <input ng-model="paph" class="info" id="pap-horiz" type="text" name="pap-horiz" placeholder="Horizontal">
				<input ng-model="papv" class="info" id="pap-updown" type="text" name="pap-updown" placeholder="Up / Down">
				<input ng-model="clt" class="info" id="pap-clt" type="text" name="pap-clt" placeholder="CLT / Offset Thumb"><br>
				Thumb: <input ng-model="thumb" class="info" id="thumb" type="text" name="thumb" placeholder="Style">
				Fingers: <input ng-model="fingers" class="info" id="fingers" type="text" name="fingers" placeholder="Style"><br>
				Price: <input ng-model="price" class="info" id="price" type="text" name="price" placeholder="Price">
				Completed by: <input ng-model="completedBy" class="info" id="completed-by" type="text" name="completed-by" placeholder="Completed by">
			</form>
		</div>
